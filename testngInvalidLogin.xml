<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="SuiteLogin" parallel="tests">
<listeners>
<listener class-name="generic.ListenerDemo"></listener>
</listeners>
  <test thread-count="5" name="TestInFirefox">
  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="scripts.TestValidLogin"/>
        <class name="scripts.TestInValidLogin"/>
    </classes>
  </test> <!-- Test -->


  <test thread-count="5" name="TestInChrome">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="scripts.TestValidLogin"/>
        <class name="scripts.TestInValidLogin"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
